<tr>
  <td class="col-md-2"><%= contract.performance.name %></td>
  <td><%= contract.performance.act.name %>  <%= link_to "#{icon('arrow-circle-right')}".html_safe, act_path(contract.performance.act) %></td>
  <td><%= contract.performance.venue.name %> <%= link_to "#{icon('arrow-circle-right')}".html_safe, venue_path(contract.performance.venue) %></td>
  <td><%= fmatdate(contract.performance.perf_date) %></td><td><%= contract.status %></td>
  <td><%= render partial: "edit_links", locals: {resource: contract, text: false, displaytype: "row"} %></td>
</tr>
