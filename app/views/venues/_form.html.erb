<%= form_for @venue do |f| %>
  <%= render partial: "shared/errors", locals: {md: @venue} %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %><br>
  <%= f.label :address %>
  <%= f.text_field :address, class: "form-control" %><br>
  <%= f.label :city %>
  <%= f.text_field :city, class: "form-control" %><br>
  <%= f.label :state %>
  <%= f.text_field :state, class: "form-control" %><br>
  <%= f.label :zipcode %>
  <%= f.text_field :zipcode, class: "form-control" %><br>
  <%= f.label :phone %>
  <%= f.phone_field :phone, class: "form-control" %><br>
  <%= f.label :email %>
  <%= f.email_field :email, class: "form-control" %><br>
  <%= f.label :website %>
  <%= f.url_field :website, class: "form-control" %><br>
  <%= f.submit %>
<% end %>
