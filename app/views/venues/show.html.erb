<%= render partial: "edit_links", locals: {resource: @venue, text: true, displaytype: "page"} %>
<h1><%= @venue.name %></h1>
<div data-performance-venue-list="<%= @venue.id %>" />
<p>
  <strong>Address:</strong><br>
  <%= @venue.address %><br>
  <%= "#{@venue.city}, #{@venue.state}  #{@venue.zipcode}" %>
</p>
<p>
  <strong>Phone: </strong><%= @venue.phone %><br>
  <strong>E-mail: </strong><%= mail_to @venue.email, @venue.email %><br>
  <strong>Website: </strong><%= link_to @venue.website, @venue.website %><br>
</p>
<p>
  &nbsp;
</p>
<h4>Users:</h4>
<div class="panel panel-default col-sm-12">
  <%= render partial: "users/user_table", locals: {collection: @users} %>
</div>
<p>
  &nbsp;
</p>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Act</th>
      <th>Venue</th>
      <th>Date</th>
      <th>Contract</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody id="upcoming_perf">
  </tbody>
  <tbody id="past_perf">
  </tbody>
</table>
